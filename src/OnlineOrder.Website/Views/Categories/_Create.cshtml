@using System.Collections
@using OnlineOrder.Mvc
@model Category


@using (Html.SiForm())
{
    @Html.SiHiddenFor(m => m.Id)  
    @Html.SiHiddenFor(m => m.ParentId)     
    @Html.SiTextBoxFor(m => m.Parent.Code,"父级类别", new { disabled = "disabled" }) 
    @Html.SiTextBoxFor(m => m.Code, new { required="true"}) 
    @Html.SiTextBoxFor(m => m.Name)  
    @Html.SiTextBoxFor(m => m.ProfitRate)  
}